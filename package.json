{
  "name": "siyuan-plugin-importer",
  "version": "1.8.0",
  "type": "module",
  "description": "Import from epub, docx, html etc. to siyuan-note",
  "repository": "terwer/siyuan-plugin-importer",
  "homepage": "https://github.com/terwer/siyuan-plugin-importer",
  "author": "terwer",
  "license": "MIT",
  "scripts": {
    "makeLink": "python scripts/make_dev_link.py",
    "serve": "vite",
    "dev": "vite build --watch",
    "build": "vite build",
    "start": "vite preview",
    "test": "vitest --watch",
    "syncVersion": "python scripts/version.py",
    "parseChangelog": "python scripts/parse_changelog.py",
    "prepareRelease": "pnpm syncVersion && pnpm parseChangelog",
    "package": "python scripts/package.py"
  },
  "devDependencies": {
    "@terwer/eslint-config-custom": "^1.3.6",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.15.31",
    "@vitejs/plugin-vue": "^5.0.4",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "siyuan": "^1.1.1",
    "stylus": "^0.64.0",
    "ts-node": "^10.9.2",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.0.2",
    "vitest": "^3.2.3",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "dependencies": {
    "shorthash2": "^1.0.5",
    "zhi-common": "^1.34.1",
    "zhi-lib-base": "^0.8.0"
  },
  "packageManager": "pnpm@10.12.1"
}